<% if user_signed_in? %>
   <%= render 'shared/navbar-loggedin' %>
<% else %>
  <div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
    <%= link_to "#", class: "navbar-brand" do %>
       <h1 id="nav-logo-long">neighbors.</h1>
      <% end %>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item d-flex" >
          <%= link_to "Log In", new_user_session_path, class: "nav-link homepage-login" %>
          <%= link_to "Sign Up", new_user_registration_path, class: "nav-link homepage-signup" %>

        </li>
      </ul>
    </div>
  </div>
<% end %>
